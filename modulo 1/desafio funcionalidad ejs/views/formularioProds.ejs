
<form action= <%= /productos %> method= <%= post %> >
    <div class="mb-3">
        <label class="form-label fw-bold" for="titulo">Titulo</label>
        <input class="form-control" name="titulo">
    </div>
    <div class="mb-3">
        <label class="form-label fw-bold" for="precio">Precio</label>
        <input class="form-control" name="precio">
    </div>
    <div class="mb-3">
        <label class="form-label fw-bold" for="miniatura">Miniatura</label>
        <input class="form-control" type="number" name="miniatura">
    </div>
    <div class="col-12">
        <input class="btn btn-success" type="submit" value="Enviar">
    </div>
</form>

<a href="/productos"><button>Volver a productos</button></a>
